{
  "name": "fest-with-me",
  "version": "1.0.0",
  "engines": {
    "node": ">= 7.6.0"
  },
  "main": "index.js",
  "scripts": {
    "dev": "webpack",
    "prod": "npm run build && npm run minify && node backend/app.js",
    "build": "cd client && webpack --config webpack.prod.config.js -p --progress --colors",
    "build:dev": "cd client && webpack --config webpack.config.js -p --progress --colors",
    "start": "cd client && webpack-dev-server",
    "postinstall": "cd client && webpack --config webpack.prod.config.js -p --progress --colors",
    "backend": "cd backend && nodemon app.js",
    "minify": "html-minifier -o ./client/dist/index.html ./client/dist/index.dev.html --minify-css true --minify-js true  --remove-comments --collapse-whitespace --collapse-inline-tag-whitespace",
    "what": "cd backend && nodemon test/utils/utilities.js",
    "test": "mocha backend/test/*.test.js"
  },
  "license": "ISC",
  "dependencies": {
    "@google/maps": "^0.4.3",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.1",
    "compression": "^1.7.2",
    "cors": "^2.8.1",
    "create-react-class": "^15.6.3",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "express": "^4.16.3",
    "geocoder": "^0.2.3",
    "html-minifier": "^3.5.11",
    "iplocation": "^5.0.0",
    "material-ui": "^0.18.3",
    "money": "^0.2.0",
    "mongoose": "^4.10.4",
    "node-fetch": "^1.7.1",
    "nuka-carousel": "^2.0.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-geolocated": "^2.0.0",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^1.13.1",
    "workbox-webpack-plugin": "^2.1.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0",
    "redux-logger": "^2.6.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.11.0"
  }
}
